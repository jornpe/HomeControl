{
    "trailingSlash": "auto",
    "routes": [
      {
        "route": "/fetchdata*",
        "allowedRoles": ["authenticated"]
      },
      {
        "route": "/counter*",
        "allowedRoles": ["registeredusers"]
      },
      {
        "route": "/api/*",
        "methods": ["GET"],
        "allowedRoles": ["registeredusers"]
      },
      {
        "route": "/login",
        "rewrite": "/.auth/login/aad"
      },
      {
        "route": "/logout",
        "redirect": "/.auth/logout"
      }
    ],
    "navigationFallback": {
      "rewrite": "index.html",
      "exclude": ["/images/*.{png,jpg,gif}", "/css/*"]
    }
  }